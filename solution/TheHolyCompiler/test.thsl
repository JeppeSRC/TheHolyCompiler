layout (location = 0) in vec3 position;

layout (binding = 0, set = 0) uniform Buffer0 {
    mat4 MVP;
};

out vec4 pos = THSL_Position;

float dot(vec3 v1, vec3 v2);

void main() {
    vec3 v1 = vec3(0.0, 1.0, 2.0);
    vec3 v2 = vec3(2.0, 1.0, 0.0);

    float a = 0.0;

    a = ++a * ++a;

    dot(v2, v2);

    v1 = v1 * v2 * v1 * v2 * v1;

    pos = Buffer0.MVP * vec4(v1 * position, 1.0);
}

float dot(vec3 v1, vec3 v2) {
    v1 *= v2;
    return v1.x + v1.y + v1.z;
}
